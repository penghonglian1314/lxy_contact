.chat_wrapper{
  border-radius:18rpx;
  padding: 10rpx;
  padding-bottom: 40rpx;
  position: relative;
  width: 100%;
  height: 100%;
  background: linear-gradient(147deg, #00EAA4 0%, #ADFFA0 100%);
}


.chat_wrapper .avatar{
  width: 147rpx;
  height: 147rpx;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%,-50%);
  border-radius: 50%;
  background-color: #F6F6F6;
  overflow: hidden;
}

.chat_wrapper .avatar image{
  width: 100%;
  height: 100%;
}

.chat_wrapper .icon_wrapper{
  height: 70rpx;
  text-align: right;
  padding: 12rpx;
}
.chat_wrapper .title{
  width: 394rpx;
  margin: 0 auto;
  padding-bottom:20rpx ;
  font-size: 29rpx;
  text-align: left;
}
.chat_wrapper .chat_btns{
  display: flex;
  flex-direction: column;
  height:240rpx;
  width: 100%;
  justify-content: space-evenly;
  align-items: center;
}
.chat_wrapper .chat_btns .chat_btn{
  color: #fff;
  font-size: 32rpx;
  width: 403rpx;
  height: 86rpx;
  line-height: 86rpx;
  font-weight: 400;
  border-radius: 43rpx;
  display: flex;
  align-items: center;
  padding-left: 8rpx;
  position: relative;
}
.chat_wrapper .chat_btns .chat_btn .chat_btn_item.img{
  flex-shrink: 0;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  position: absolute;
}
.chat_wrapper .chat_btns .chat_btn .chat_btn_item.text{
  flex: 1;
  text-align: center;
}
.chat_wrapper .chat_btns .chat_btn:first-child{
background-color: #FFD300;
color: #fff;
}
.chat_wrapper .chat_btns .chat_btn:nth-child(2){
  background-color: #003D2B;
  /* opacity: 0.39; */
  color: #fff;
}
