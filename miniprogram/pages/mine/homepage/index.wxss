}

.noimage1{
  color: gray;
}

.body .photo .masklayer{
  position:absolute;
  top: 0;
  /* z-index: 10; */
  background: #dfdfdf;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.masklayer view{
  z-index: 1;
  color: white;
}

.masklayer image{
  width: 100%;
  position: absolute;
  top: 0px;
  left: 0px;
  filter: blur(10px);
}

.masklayer .masklayer1 button{
  border: 1px solid white;
}

.masklayer2 button van-icon view{
  width: 40rpx;
  height: 40rpx;
}

.masklayer2 button van-icon view image{
  filter: none;
}

.body .photo .haveimg{
