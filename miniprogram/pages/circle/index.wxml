  </view>

  <show id="show"></show>

  <view class="header-right" >
    <!-- <van-icon name="filter-o" size="20px" wx:if="{{isshow}}" bindtap = "selectClick"/> -->
    <image src="../../images/sx.png" wx:if="{{isshow}}" bindtap = "selectClick"></image>
  </view>
  <van-action-sheet show="{{ SelectShow }}" bind:close="SelectClose" title="筛选" wx:if="{{islady}}">
    <view style="padding-top:30rpx;padding-left:20rpx;">想看的用户</view>
    <view class="btngroup">
      <a class="{{btnactive == 1 && 'btnactive' }}"  bindtap="selall">全部</a>
      <a class="{{btnactive == 2 && 'btnactive' }}" bindtap="selnew">新人</a>
      <a class="{{btnactive == 3 && 'btnactive' }}" bindtap="selbeautiful">女神</a>
    </view>
    <view class="online" >
      <view>
        只看在线
      </view>
      <van-switch checked="{{ online }}" bind:change="onlineChange" size="50rpx" active-color="#00EAA4"/>
    </view>
    <view class="bottombtn">
      <van-button class="but_cz1" color="whitesmoke" round="true" type="default" bindtap="but_cz">重置</van-button>
      <van-button class="but_cz2" color="#FF0055" round="true" type="default" bindtap="but_sx">确定</van-button>
    </view>
  </van-action-sheet>
  <van-action-sheet show="{{ SelectShow }}" bind:close="SelectClose" title="筛选" wx:else>
    <view style="padding-top:30rpx;padding-left:20rpx;">想看的用户</view>
    <view class="btngroup">
      <a class="{{btnactive1 == 1 && 'btnactive' }}"  bindtap="selall1">全部</a>
      <a class="{{btnactive1 == 2 && 'btnactive' }}" bindtap="selVIP">VIP</a>
      <a class="{{btnactive1 == 3 && 'btnactive' }}" bindtap="selSVIP">SVIP</a>
    </view>
    <view class="online" >
      <view>
        只看在线
      </view>
      <van-switch checked="{{ online1 }}" bind:change="onlineChange1" size="50rpx" active-color="#00EAA4"/>
    </view>
    <view class="bottombtn">
      <van-button class="but_cz1" color="whitesmoke" round="true" type="default" bindtap="but_cz1">重置</van-button>
      <van-button class="but_cz2" color="#FF0055" round="true" type="default" bindtap="but_sx1">确定</van-button>
    </view>
  </van-action-sheet>
</view>

