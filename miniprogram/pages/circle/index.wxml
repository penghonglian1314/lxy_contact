<view class="custom-bar" style="height:{{navHeight}}px">
  <view class="custom-bar__wrapper" style="margin-top:{{searchMarginTop}}px; height: {{searchHeight}}px;width: {{searchWidth}}px" >
    <view class="search-group">
      <van-icon name="search" size="25px" style="margin-left:20rpx" bindtap = "searchClick"/>
      <view class="title">
        圈子
      </view>
        
    </view>
  </view>
</view>

<view style="height:{{navHeight}}px"></view>

<view class="circle" bindtap = "close">
  <view class="header-left">
    <van-tabs active="{{ active }}" swipe-threshold="7"  color="#D9D9D9" bind:change="onChange" id="tabs">
      <van-tab title="全部">
        <!-- <all/> -->
        <view>
          <view class="list" hover-class="none" hover-stop-propagation="false" wx:for="{{all}}" wx:key="key">
            <view class="left" >
              <view class="img" data-index="{{item.MemberID}}" bindtap="but1">
                <image mode="aspectFill" src="{{item.AvatarUrl}}"></image>
                <van-tag color="rgba(0,0,0,0.5)"  text-color="#ADADAD">{{item.PhotoUrl}}</van-tag>
              </view>
              
            </view>
            <view class="right" >
              <view class="info" >
                <view class="top" hover-class="none" hover-stop-propagation="false">
                  <view selectable="false" space="false" decode="false">
                    {{item.MemberName}}
                  </view>
                  
                  <view class="con_tet3">
                    <image mode="aspectFit" src="{{item.Level==0? '../../images/xr.png':'../../images/vip.png'}}"></image>
                  </view>
                  <!-- <van-tag color="#E1E1E1" size="medium" text-color="#ADADAD">会员</van-tag>
                  <van-tag color="#E1E1E1" size="medium" text-color="#ADADAD">新人</van-tag> -->
                </view>
                <view class="center" hover-class="none" hover-stop-propagation="false">
                  <view class="con_tet4 {{item.Gender==0? 'con_tet4_1':'con_tet4_2'}}">
                    <view decode='true'>{{item.Gender==0? '♂':'♀'}}</view>
                    <text>{{item.Birth}}</text>
                  </view>

                  <view wx:if="{{item.PhotoType==1}}" class="con_tet4 {{item.Gender==0? 'con_tet4_1':'con_tet4_2'}} con_tet4_3">
                    <!-- <view decode='true'>{{item.Gender==0? '♂':'♀'}}</view> -->
                    <text>相册锁</text>
                  </view>

                  <view wx:if="{{item.Constellation!=''}}" class="con_tet4 con_tet4_3">
                    <!-- <view decode='true'>{{item.Gender==0? '♂':'♀'}}</view> -->
                    <text>{{item.Constellation}}</text>
                  </view>
                  <!-- <van-tag color="#E1E1E1" size="medium" text-color="#ADADAD">相册锁</van-tag> -->
                </view>
                <view class="bottom" hover-class="none" hover-stop-propagation="false">
                  {{item.City!=''? item.City : '--'}}<text decode='true'>{{item.introduce!=''? '&nbsp;'+'·'+'&nbsp;'+item.introduce: ''}}</text>
                </view>
              </view>
              <view class="more" >
                <view class="top" hover-class="none" hover-stop-propagation="false">
                  <view wx:if="{{item.Online==0}}"></view>

                  {{item.Online=='0'? '在线':item.Online}}
                </view>
                <van-icon name="ellipsis" id="{{item.id}}" data-item="{{item}}"  catchtap = "onTap"/>
              </view>
            </view>

            <popover id='popover' totop="115">
              <popover-item catchtap='onClickA' hasline>
              <image class="popover1" src="../../images/jrxh.png"></image>
              加入喜欢</popover-item>
              <popover-item catchtap='blacklist' hasline>
                <image class="popover1" src="../../images/lh.png"></image>
              拉黑</popover-item>
           </popover>
          </view>
        </view>
      </van-tab>
