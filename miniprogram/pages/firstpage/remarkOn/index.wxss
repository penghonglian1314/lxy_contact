/* 评论-回复-回复评论显示区域 */
.container{ 
  /* margin-top: 39rpx; */
  margin-bottom:100rpx;
  padding: 0;
  display: unset;
}
.container-item{ 
  width: 100%; 
  display: flex;  
  margin-bottom: 34rpx;  
  align-items: flex-start;
}
.item_img{  
  width: 64rpx;  
  height: 64rpx; 
  border-radius: 50%; 
  margin-right: 17rpx;
}
.item_right{ 
  width: 86%; 
  display: inline-block;  
  border-bottom: 2rpx solid #E5E5E5;  
  padding-bottom: 40rpx;
}
.right_name{  
  font-size: 24rpx;  
  color: #999999;
  float: left;
}

.right_name1{
  width: 70rpx;
  height: 30rpx;
  background: #FFF2F6;
  font-size: 12px;
  color: #FF5697;
  float: left;
  border-radius: 20px;
  text-align: center;
  line-height: 30rpx;
  font-weight: bold;
}

.right_content{   
  margin-top: 20rpx;
}
.right_content_txt{  
  font-size: 26rpx;  
  color: #333333;
}
.right_content_txt2{  
  font-size: 24rpx;  
  color: #CCCCCC;  
  margin-left: 30rpx;
  float: right;
}
.reply{  
  display: flex;   
  margin-top: 32rpx;  
  align-items: flex-start;  
}
.reply_img{  
  width: 40rpx;  
  height: 40rpx;
  border-radius: 50%;  
  margin-right: 17rpx;
}
.reply_right{  
  display: inline-block;  
  width: 546rpx;
}

